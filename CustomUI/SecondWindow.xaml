<Window x:Class="CustomUI.SecondWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomUI"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="None" AllowsTransparency="True"
        Title="MainWindow" Height="640" Width="360">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <local:TitleBar
            x:Name="MainTitleBar"
            TitleText="SecondWindow"
            BackIcon="{StaticResource BackIcon}"
            MenuIconVisibility="Collapsed" />
        <TextBlock
            x:Name="SecondBtn" Grid.Row="1" Text="This is Second Window"
            VerticalAlignment="Center" HorizontalAlignment="Center"
            FontSize="20"/>
    </Grid>
</Window>